{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/posts",
      "dest": "/server.js"
    },
    {
      "src": "/styles.css",
      "dest": "/server.js"
    },
    {
      "src": "/app.js", 
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}